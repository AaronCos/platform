<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sswh.platform.dao.UserDao">
    <insert id="registUser" parameterType="com.sswh.platform.entity.User" >
                INSERT INTO platform_user(name,password)
        VALUES(#{name},#{password});
    </insert>
</mapper>